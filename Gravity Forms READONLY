<script type="module">
    const input = document.getElementById('input_2_5').value;
    const name = document.getElementById('input_2_5');
    const last = document.getElementById('input_2_6');
    const email = document.getElementById('input_2_1');
    const phone0 = document.getElementById('input_2_7');
    const phone1 = document.getElementById('input_2_8');
    const secphone = document.getElementById('input_2_8').value;

    if ( input != '' ) {
    name.setAttribute('readonly', true);
    last.setAttribute('readonly', true);
    email.setAttribute('readonly', true);
    phone0.setAttribute('readonly', true);
  }
    if ( secphone != '' ) {
    phone1.setAttribute('readonly', true);
  }
</script>
